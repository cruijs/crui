{
  "compilerOptions": {
    "declaration": true,
    "target": "esNext",
    "module": "esNext",
    "moduleResolution": "node",
    "noEmit": true,
    "sourceMap": true,
    "outDir": "./dist/",
    "strict": true,
    "strictNullChecks": true,
    "alwaysStrict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "allowSyntheticDefaultImports": true
  }
}